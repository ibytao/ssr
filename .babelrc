{
  "presets": [
    "es2015",
    "react",
    "stage-0",
    "stage-2"
  ],
  "env": {
    "production": {
      "presets": ["react-optimize"]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel",
        ["import", { libraryName: "antd-mobile" }] // `style: true` 会加载 less 文件
      ]
    }
  }
}
